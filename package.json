{
  "name": "playable-template-pixi",
  "version": "1.0.0",
  "description": "A starter template for creating playable ads using PixiJS and @smoud/playable-sdk with TypeScript support",
  "scripts": {
    "dev": "playable-scripts dev",
    "build": "playable-scripts build",
    "build:en": "playable-scripts build --language en",
    "build:de": "playable-scripts build --language de",
    "build:all": "npm run build:en && npm run build:de"
  },
  "keywords": [
    "playable",
    "ads",
    "template",
    "pixi",
    "pixijs",
    "webgl",
    "2d",
    "typescript",
    "game"
  ],
  "author": "Smoud",
  "dependencies": {
    "@pixi/sound": "^6.0.1",
    "@smoud/playable-sdk": "^1.0.23",
    "gsap": "^3.14.2",
    "pixi.js": "^8.8.1"
  },
  "devDependencies": {
    "@assetpack/core": "^1.7.0",
    "@smoud/playable-scripts": "^1.1.0",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
